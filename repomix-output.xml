    transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
    transition-duration: 150ms;
  }
  
  .duration-300 {
    transition-duration: 300ms;
  }
  
  /* Overflow */
  .overflow-hidden {
    overflow: hidden;
  }
  
  /* Object Fit */
  .object-cover {
    object-fit: cover;
  }
  
  /* Fill */
  .fill-current {
    fill: currentColor;
  }
  
  /* Resize */
  .resize-none {
    resize: none;
  }
  
  /* Outline */
